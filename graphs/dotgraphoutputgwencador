digraph finite_state_machine {

    rankdir=LR;

    node [shape = circle];
	4 -> 5[label = 6];
	4 -> 2[label = 8];
	5 -> 4[label = 8];
	5 -> 1[label = 7];
	2 -> 4[label = 4];
	2 -> 0[label = 8];
	0 -> 3[label = 10];
	3 -> 4[label = 5];
	3 -> 2[label = 2];
	3 -> 1[label = 11];
	1 -> 4[label = 1];
	1 -> 0[label = 7];
	1 -> 5[label = 14];
}