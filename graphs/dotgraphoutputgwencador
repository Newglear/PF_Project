digraph finite_state_machine {

    rankdir=LR;

    node [shape = circle];
	1 -> 5[label = 14];
	1 -> 0[label = 7];
	1 -> 4[label = 1];
	5 -> 1[label = 7];
	0 -> 3[label = 10];
	0 -> 2[label = 8];
	4 -> 5[label = 14];
	3 -> 1[label = 11];
	3 -> 2[label = 2];
	3 -> 4[label = 5];
	2 -> 4[label = 12];
}